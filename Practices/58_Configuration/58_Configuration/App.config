<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	
	<configSections>
		<section name="connectionSetting" type="System.Configuration.NameValueSectionHandler"/>
	</configSections>
	
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	
	<appSettings>
		<add key="projectName" value="renovation"/>
		<add key="version" value="1234"/>
	</appSettings>
	
	<connectionStrings>
		<add name="firstDB" connectionString="SQLSever" />
	</connectionStrings>

	<connectionSetting>
		<add key="timeout" value="2000"/>
		<add key="retryCount" value="5"/>
	</connectionSetting>
</configuration>