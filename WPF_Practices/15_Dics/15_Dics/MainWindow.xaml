<Window x:Class="_15_Dics.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_15_Dics"
        xmlns:views="clr-namespace:_15_Dics.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <views:StackView x:Name="Stack1" Grid.Column="0"/>
        <ContentControl Name="ccMain2" Grid.Column="1"/>
        <ContentControl Name="ccMain3" Grid.Column="2"/>
        <Button Name="btnButton1" Grid.Row="1" Grid.Column="0" Click="btnButton1_Click"/>
    </Grid>
</Window>
