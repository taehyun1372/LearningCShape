<Window x:Class="_14_Routed_Events.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_14_Routed_Events"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Name="MyPanel" Height="50" Orientation="Horizontal" local:CustomTextBox.TwentyDetected="MyPanel_TwentyDetected" ButtonBase.Click="StackPanel_Click">
            <Label Content="First" Margin="10"/>
            <Button Content="Second" Margin="10"/>
            <Button Content="Third" Margin="10"/>
            <local:CustomTextBox Width="50"/>
            <local:CustomTextBox Width="50"/>
        </StackPanel>
    </Grid>
</Window>
